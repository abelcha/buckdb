{
    "workbench.colorTheme": "Default Dark+",
    "workbench.iconTheme": "vs-seti",
    "editor.autoClosingBrackets": "languageDefined",
    "editor.autoClosingQuotes": "languageDefined",
    "editor.scrollBeyondLastLine": true,
    "editor.mouseWheelZoom": true,
    "editor.wordBasedSuggestions": "off",
    "workbench.panel.showLabels": false,
    "editor.acceptSuggestionOnEnter": "on",
    "window.confirmBeforeClose": false,
    "editor.foldingHighlight": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.bracketPairColorization.enabled": false,
    "editor.fontSize": 12,
    "accessibility.signals.lineHasError": {
        "sound": "on"
    },
    "accessibility.signals.onDebugBreak": {
        "sound": "on"
    },
    "files.autoSave": "off",
    "debug.toolBarLocation": "docked",
    "editor.experimental.asyncTokenization": true,
    "terminal.integrated.tabs.title": "${sequence}",
    "typescript.tsserver.log": "normal",
    "workbench.sideBar.location": "left",
    "editor.experimental.preferTreeSitter": [
        "typescript"
    ],
    "workbench.colorCustomizations": {
        "editor.background": "#0d0d0d",
        "editor.foreground": "#f2f2f2",
        "sideBar.background": "#0d0d0d",
        "sideBar.foreground": "#d9d9d9",
        "sideBar.border": "#333333",
        "activityBar.background": "#0d0d0d",
        "activityBar.foreground": "#fafafa",
        "activityBar.inactiveForeground": "#999999",
        "activityBar.border": "#333333",
        "panel.background": "#0d0d0d",
        "panel.border": "#333333",
        "panelTitle.activeForeground": "#f2f2f2",
        "panelTitle.inactiveForeground": "#b2b2b2",
        "statusBar.background": "#1a1a1a",
        "statusBar.foreground": "#d9d9d9",
        "statusBar.border": "#333333",
        "statusBar.noFolderBackground": "#1a1a1a",
        "statusBarItem.remoteBackground": "#1a1a1a",
        "titleBar.activeBackground": "#1a1a1a",
        "titleBar.activeForeground": "#fafafa",
        "titleBar.inactiveBackground": "#1a1a1a",
        "titleBar.inactiveForeground": "#b2b2b2",
        "titleBar.border": "#333333",
        "tab.activeBackground": "#0d0d0d",
        "tab.inactiveBackground": "#1a1a1a",
        "tab.activeForeground": "#fafafa",
        "tab.inactiveForeground": "#b2b2b2",
        "tab.border": "#333333",
        "editorGroupHeader.tabsBackground": "#1a1a1a",
        "list.hoverBackground": "#1a1a1a",
        "list.activeSelectionBackground": "#2f2277",
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionBackground": "#262626",
        "list.highlightForeground": "#fff100",
        "input.background": "#262626",
        "input.foreground": "#f2f2f2",
        "input.border": "#333333",
        "dropdown.background": "#262626",
        "dropdown.border": "#333333",
        "button.background": "#4c4c4c",
        "button.foreground": "#fafafa",
        "button.hoverBackground": "#666666",
        "scrollbarSlider.background": "#333333",
        "scrollbarSlider.hoverBackground": "#4c4c4c",
        "scrollbarSlider.activeBackground": "#666666",
        "editorLineNumber.foreground": "#808080",
        "editorWidget.background": "#1a1a1a",
        "editorWidget.border": "#333333",
        "textLink.foreground": "#2eafff",
        "errorForeground": "#ff0000",
        "focusBorder": "#fff100",
        "progressBar.background": "#fff100"
    }
}