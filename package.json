{
  "name": "buckdb",
  "version": "0.1.10",
  "type": "module",
  "main": "./node.ts",
  "module": "./node.ts",
  "exports": {
    ".": "./node.ts",
    "./tf": "./tf.ts",
    "./core": "./core.ts",
    "./fn": "./fn.ts",
    "./node": "./node.ts",
    "./io": "./io.ts",
    "./wasm": "./wasm.ts",
    "./remote": "./remote.ts"
  },
  "files": [
    ".buck/*",
    "src/*.ts",
    "*.ts",
    "!src/*.test.ts"
  ],
  "scripts": {
    "demo": "npm start --prefix demo",
    "bump": "deno run --allow-all jsr:@kellnerd/bump --file jsr.json --commit ",
    "patch": "bun run bump && jsr publish --allow-slow-types --allow-dirty --token $JSR_TOKEN",
    "typecheck": "tsc --noEmit --skipLibCheck --resolveJsonModule --moduleResolution Node --module es2022 --allowSyntheticDefaultImports -p tsconfig.typecheck.json"
  },
  "devDependencies": {
    "@types/bun": "^1.2.5"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@duckdb/node-api": "^1.4.1-r.4",
    "es-toolkit": "^1.33.0",
    "typescript": "5.9.2"
  },
  "license": "MIT"
}
